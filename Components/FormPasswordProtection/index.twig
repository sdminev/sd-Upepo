<flynt-component name="FormPasswordProtection" class="componentSpacing" data-theme="dark">
  <div class="centerMaxWidthContainer">
    <div class="centerContentMaxWidth">
      <div class="boxRadius boxPadding boxShadow" data-theme="reset">
        {{ contentHtml|e('wp_kses_post') }}
        <br>
        <form action="{{ form.url }}" method="post">
            <input name="post_password" type="password" size="30" maxlength="30" aria-label="{{ __('Password', 'flynt') }}" placeholder="{{ _x('Enter password', 'password form placeholder', 'flynt') }}" />
            <br><br>
            <button class="button oneInputForm-button" type="submit" name="Submit">{{ _x('Enter', 'post password form', 'flynt') }}</button>
        </form>
      </div>
    </div>
  </div>
</flynt-component>
